@prefix darpa: <http://ss.l/dp#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .	
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sigma: <http://w3id.org/sepses/vocab/rule/sigma#> .
@prefix cat: <http://w3id.org/sepses/vocab/ref/cat#> .
@base <http://ss.l/dp> .

<http://ss.l/dp> a owl:Ontology .

darpa:Process a rdfs:Class.

darpa:File a rdfs:Class.

darpa:Executable a rdfs:Class.

darpa:Network a rdfs:Class.
      
darpa:Host a rdfs:Class.

darpa:IPAddress a rdfs:Class.

darpa:User a rdfs:Class.

darpa:RootUser a rdfs:Class;
	rdfs:subClassOf darpa:User.

darpa:LocalUser a rdfs:Class;
      	rdfs:subClassOf darpa:User.

darpa:SystemUser a rdfs:Class;
	rdfs:subClassOf darpa:User.

#=====property===================

darpa:forks a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Process ;
    rdfs:range darpa:Process.

darpa:isExecutedBy a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:File ;
    rdfs:range darpa:Process .

darpa:isReadBy a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:File ;
    rdfs:range darpa:Process .

darpa:writes a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Process ;
    rdfs:range darpa:File .    

darpa:isReceivedBy a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Network ;
    rdfs:range darpa:Process .

darpa:sends a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Process ;
    rdfs:range darpa:Network .    

darpa:hasCmd a rdf:Property;
   rdfs:domain darpa:Process ;
   rdfs:range xsd:string.

darpa:hasExe a rdf:Property;
   rdfs:domain darpa:Process ;
   rdfs:range darpa:Executable.

darpa:hasUser a rdf:Property;
   rdfs:domain darpa:Process ;
   rdfs:range darpa:User.

darpa:userName a rdf:Property;
   rdfs:domain darpa:User ;
   rdfs:range xsd:string.

darpa:userId a rdf:Property;
   rdfs:domain darpa:User ;
   rdfs:range xsd:integer.

darpa:hasHost a rdf:Property;
   rdfs:domain darpa:Process ;
   rdfs:range darpa:Host.

darpa:hostName a rdf:Property;
   rdfs:domain darpa:Host ;
   rdfs:range xsd:string.

darpa:hostOS a rdf:Property;
   rdfs:domain darpa:Host ;
   rdfs:range xsd:string.

darpa:IPAddress a rdf:Property;
   rdfs:domain darpa:Host ;
   rdfs:range  xsd:string.

#darpa:isIllegallyExecutedBy a rdf:Property;
#    rdfs:subPropertyOf darpa:alertAccess ;
#    rdfs:domain darpa:File ;
#    rdfs:range darpa:Process .

#darpa:leaksDataTo a rdf:Property;
#    rdfs:subPropertyOf darpa:alertAccess ;
#    rdfs:domain darpa:Process ;
#    rdfs:range darpa:Network .

darpa:hasIPAddress a rdf:Property;
    rdfs:domain darpa:Network;
    rdfs:range darpa:IPAddress.

darpa:port a rdf:Property;
    rdfs:domain darpa:Network;
    rdfs:range xsd:integer.

#==================additional ontology===============

cat:Technique a rdfs:Class.

sigma:SigmaRule a rdfs:Class.

darpa:hasPotentialAttackTechnique a rdf:Property;
    rdfs:domain darpa:Process ;
    rdfs:range cat:Technique .

darpa:hasDetectedRule a rdf:Property;
    rdfs:domain darpa:Process ;
    rdfs:range sigma:SigmaRule .

