@prefix darpa: <http://ss.l/dp#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 	
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

darpa:Process a rdfs:Class .
darpa:FileObject a rdfs:Class .
darpa:Network a rdfs:Class .

darpa:forks a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Process ;
    rdfs:range darpa:Process.

darpa:isExecutedBy a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:FileObject ;
    rdfs:range darpa:Process .

darpa:isReadBy a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:FileObject ;
    rdfs:range darpa:Process .

darpa:writes a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Process ;
    rdfs:range darpa:FileObject .    


darpa:isReceivedBy a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Network ;
    rdfs:range darpa:Process .

darpa:sends a rdf:Property;
    rdfs:subPropertyOf darpa:connects ;
    rdfs:domain darpa:Process ;
    rdfs:range darpa:Network .    


darpa:isIllegallyExecutedBy a rdf:Property;
    rdfs:subPropertyOf darpa:alertAccess ;
    rdfs:domain darpa:FileObject ;
    rdfs:range darpa:Process .


darpa:leaksDataTo a rdf:Property;
    rdfs:subPropertyOf darpa:alertAccess ;
    rdfs:domain darpa:Process ;
    rdfs:range darpa:Network .

darpa:connects a rdf:Property .

darpa:leaksDataTo a rdf:Property .

darpa:isIllegallyExecutedBy a rdf:Property .



